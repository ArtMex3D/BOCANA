<?xml version="1.0" encoding="utf-8"?>
<!--
  Este archivo define una forma que es una línea.
  Se usa un 'stroke' (trazo) para crear la línea punteada.
-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">

    <stroke
        android:width="2dp"
        android:color="@android:color/black"
        android:dashWidth="8dp"
        android:dashGap="4dp" />

</shape>
